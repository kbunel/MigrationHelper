{
    "name": "kbunel/migration-helper-sf4",
    "type": "symfony-bundle",
    "description": "This bundle helps you migrating your application by moving all files in your application in a restructured architecture from SF3 to SF4",
    "keywords": [ "migration", "architecture", "sf4", "Symfony", "Symfony 4" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bunel",
            "email": "kevinbunel@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "kbunel/file-analyzer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "MigrationHelperSF4\\": "./"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "/Users/kbo/Documents/Projects/FileAnalyzer"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    }
}
